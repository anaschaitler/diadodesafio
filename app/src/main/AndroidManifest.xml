<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="br.edu.utfpr.diadodesafio">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".view.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".view.UsuariosActivity" />
        <activity android:name=".view.IniciarMonitoramentoActivity" />
        <activity android:name=".view.MeuPerfilActivity" />
        <activity android:name=".view.MostrarGruposActivity" />
        <activity android:name=".view.VizualizarGrupoActivity" />
        <activity android:name=".view.LoginActivity" />
        <activity android:name=".view.ListaGrupoActivity" android:theme="@style/AppTheme.NoActionBar"/>
        <activity android:name=".view.CadastroGrupoActivity"></activity>
    </application>

</manifest>