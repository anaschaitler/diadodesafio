    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.GruposActivity" />
        <activity android:name=".activity.UsuariosActivity" />
        <activity android:name=".activity.IniciarMonitoramentoActivity" />
        <activity android:name=".activity.MeuPerfilActivity" />
        <activity android:name=".activity.NovoGrupoActivity" />
        <activity android:name=".activity.MostrarGruposActivity" />
        <activity android:name=".activity.VizualizarGrupoActivity" />
        <activity android:name=".activity.LoginActivity"></activity>
    </application>

</manifest>